{
  "name": "co-lab",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd packages/db && npx prisma generate --schema=prisma/schema.prisma && cd ../..",
    "start-user-app": "cd ./apps/web && npm run dev"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "next-auth": "^4.24.7",
    "prisma": "^5.19.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0"
  }
}
